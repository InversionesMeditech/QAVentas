                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        SUM(CASE WHEN ISNULL(B.comprobante, '') = '' THEN 1 ELSE 0 END) AS PorPagar, SUM(CASE WHEN ISNULL(B.comprobante, '') &lt;&gt; '' THEN 1 ELSE 0 END) 
                         AS Pagado
FROM            { oj Header_Pago A LEFT OUTER JOIN
